<% content_for :guide_heading do %>
  <%= render "govuk_publishing_components/components/heading", {
      text: presented_manual.title,
      font_size: "l",
      inverse: true,
      id: "manual-title",
      heading_level: 1,
      margin_bottom: 6,
  } %>
<% end %>
<%=
  render(
    'content_for_head_tag',
    description: presented_manual.summary,
    title: presented_document.full_title,
  )
%>
<%= render 'header', presented_manual: presented_manual %>
<%=
  render(
    'manual_breadcrumbs',
    presented_manual: presented_manual,
    presented_document: presented_document
  )
%>

<div class="govuk-grid-row">
  <%=
    render(
      'manual_section',
      presented_manual: presented_manual,
      presented_document: presented_document
    )
  %>
</div>
